


generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model Products {
  id Int @default(autoincrement()) @id
  name String
  price Float

  available Boolean @default(true)


  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([available])
}
